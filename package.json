{
  "name": "node-billing-engine",
  "version": "1.0.0",
  "description": "An example Node.js app ",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node -r dotenv/config index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:migrate:make": "node -r dotenv/config ./node_modules/.bin/knex migrate:make --esm --knexfile ./config/knexfile.js",
    "db:migrate:latest": "node -r dotenv/config ./node_modules/.bin/knex migrate:latest --esm --knexfile ./config/knexfile.js",
    "db:migrate:rollback": "node -r dotenv/config ./node_modules/.bin/knex migrate:rollback --esm --knexfile ./config/knexfile.js",
    "scripts:create-api-key": "babel-node -r dotenv/config scripts/create_api_key.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/przemyslawhaptas/node-billing-engine.git"
  },
  "author": "Przemysław Haptaś",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/przemyslawhaptas/node-billing-engine/issues"
  },
  "homepage": "https://github.com/przemyslawhaptas/node-billing-engine#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-joi-validation": "^5.0.0",
    "express-react-views": "^0.11.0",
    "helmet": "^4.6.0",
    "joi": "^17.4.0",
    "knex": "^0.95.7",
    "monet": "^0.9.2",
    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.3"
  }
}
